# Server runtime
PORT=8000
ALLOWED_ORIGINS=http://localhost:3000

# Analytics admin
ANALYTICS_ADMIN_PASSWORD=change-me

# Validation behavior
STRICT_SERVER_VALIDATION=false
ENABLE_GUESS_FSTS=false

# Analytics storage
